{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/generate-pdf",
      "dest": "/api/generate-pdf"
    },
    {
      "src": "/api/generate-multi-page-pdf",
      "dest": "/api/generate-multi-page-pdf"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
